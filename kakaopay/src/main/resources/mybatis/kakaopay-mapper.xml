<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kakaopay">
	
	<select id="origin" resultType="long">
		select kakaopay_value from kakaopay
	</select>
	
	<update id="update">
		update kakaopay set kakaopay_value = #{kakaopayValue} where kakaopay_owner = #{kakaopayOwner}
	</update>
	
</mapper>